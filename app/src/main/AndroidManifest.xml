<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bookkeeping">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="记账本"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme_Blue"
        android:usesCleartextTraffic="true">
        <activity android:name=".my.My"></activity>
        <activity android:name=".bookcalendar.BookCalendar" />
        <activity android:name=".chart.BookChart" />
        <activity android:name=".singin.SetPwd" />
        <activity android:name=".singin.Register" />
        <activity android:name=".singin.Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:windowSoftInputMode="adjustPan" />
    </application>

</manifest>